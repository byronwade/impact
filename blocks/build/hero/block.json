{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wades/hero",
  "version": "0.1.0",
  "title": "Hero Section",
  "category": "wades-blocks",
  "icon": "cover-image",
  "description": "A hero section with heading, subheading, and call-to-action buttons",
  "attributes": {
    "heading": {
      "type": "string",
      "default": ""
    },
    "subheading": {
      "type": "string",
      "default": ""
    },
    "primaryCtaText": {
      "type": "string",
      "default": ""
    },
    "primaryCtaLink": {
      "type": "string",
      "default": ""
    },
    "secondaryCtaText": {
      "type": "string",
      "default": ""
    },
    "secondaryCtaLink": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "align": [
      "full",
      "wide"
    ],
    "html": false,
    "anchor": true,
    "color": {
      "background": true,
      "text": true,
      "gradients": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "textdomain": "wades",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}